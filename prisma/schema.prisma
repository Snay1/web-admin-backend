generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model OzonKeys {
  id       Int    @id @default(autoincrement())
  apiKey   String
  clientId String
}

model WbKeys {
  id       Int    @id @default(autoincrement())
  headerApiKey   String
}

