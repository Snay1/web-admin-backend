generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model OzonKeys {
  id       Int    @id @default(autoincrement())
  apiKey   String
  clientId String
}

model WbKeys {
  id           Int    @id @default(autoincrement())
  headerApiKey String
}

model WbBarcodes {
  id    Int      @id @default(autoincrement())
  nmID  Int      @unique
  items String[]
}

model AvitoKeys {
  id            Int    @id @default(autoincrement())
  client_id     String
  client_secret String
}

model YandexMarketKeys {
  id            Int    @id @default(autoincrement())
  client_id     String
  client_secret String
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
  hash  String
  salt  String
}
